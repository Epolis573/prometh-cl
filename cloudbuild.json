{
    "steps": [
      {
        "name": "gcr.io/cloud-builders/npm",
        "args": ["install"]
      },
      {
        "name": "gcr.io/cloud-builders/npm",
        "args": ["run", "build"]
      },
      {
        "name": "gcr.io/cloud-builders/gcloud",
        "args": [
          "run",
          "deploy",
          "my-service",
          "--image",
          "gcr.io/avid-influence-454106-d5/my-service",
          "--platform",
          "managed",
          "--region",
          "us-central1",
          "--allow-unauthenticated"
        ]
      }
    ],
    "images": ["gcr.io/avid-influence-454106-d5/my-service"],
    "timeout": "600s"
  }
  